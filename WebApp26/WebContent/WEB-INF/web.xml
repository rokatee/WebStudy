<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
		xmlns="http://xmlns.jcp.org/xml/ns/javaee" 
		xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" 
		version="3.1">
  <display-name>WebApp26</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  
  <!-- MVCBegin -->
	<servlet>
		<servlet-name>MVCBegin</servlet-name>
		<servlet-class>com.test.svt.MVCBegin</servlet-class>
	</servlet>
		
	<servlet-mapping>
		<servlet-name>MVCBegin</servlet-name>
		<url-pattern>/mvcbegin</url-pattern>
	</servlet-mapping>
  
  <!-- MVCTest01 -->
	<servlet>
		<servlet-name>MVCTest01</servlet-name>
		<servlet-class>com.test.svt.MVCTest01</servlet-class> 
	</servlet>
		
	<servlet-mapping>
		<servlet-name>MVCTest01</servlet-name> 
		<url-pattern>/mvctest1</url-pattern>
	</servlet-mapping>
  
  <!-- MVCTest02 -->
	<servlet>
		<servlet-name>MVCTest02</servlet-name>
		<servlet-class>com.test.svt.MVCTest02</servlet-class> 
	</servlet>
		
	<servlet-mapping>
		<servlet-name>MVCTest02</servlet-name> 
		<url-pattern>/mvctest2</url-pattern>
	</servlet-mapping>
  
</web-app>